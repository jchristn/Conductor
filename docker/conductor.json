{
  "Webserver": {
    "Hostname": "*",
    "Port": 9000,
    "Ssl": false,
    "Cors": {
      "Enabled": true,
      "AllowedOrigins": ["*"],
      "AllowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "AllowedHeaders": ["Content-Type", "Authorization", "x-tenant-id", "x-email", "x-password"],
      "ExposedHeaders": [],
      "AllowCredentials": false,
      "MaxAgeSeconds": 86400
    }
  },
  "Database": {
    "Type": "Sqlite",
    "Filename": "/app/data/conductor.db"
  },
  "Logging": {
    "Servers": [],
    "LogDirectory": "/app/logs/",
    "LogFilename": "conductor.log",
    "ConsoleLogging": true,
    "EnableColors": false,
    "MinimumSeverity": 0
  },
  "AdminApiKeys": [
    "conductoradmin"
  ]
}
